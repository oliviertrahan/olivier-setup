{
    "vim.vimrc.enable": true,
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["<Space>", "t", "c"],
            "commands": [":tabclose<CR>"]
        },
        {
            "before": ["<Space>", "t", "o"],
            "commands": [":tabnew<CR>"]
        },
        {
            "before": ["<Space>", ":"],
            "commands": [
                "workbench.action.showCommands",
            ],
            "silent": true
        },
        {
            "before": ["<Space>", "t", "c"],
            "commands": [":tabclose<CR>"]
        },
        {
            "before": ["<Space>", "t", "p"],
            "commands": [":tabonly<CR>"]
        },
        {
            "before": ["<Space>", "t", "l"],
            "commands": [":tabnext<CR>"]
        },
        {
            "before": ["<Space>", "t", "h"],
            "commands": [":tabprevious<CR>"]
        },
        {
            "before": ["<Space>", "t", "n"],
            "commands": [":+tabmove<CR>"]
        },
        {
            "before": ["<Space>", "t", "N"],
            "commands": [":-tabmove<CR>"]
        },
        {
            "before": ["<Space>", "f", "g"],
            "commands": ["workbench.action.findInFiles"]
        },
        {
            "before": ["<Space>", "f", "f"],
            "commands": ["workbench.action.FindInPath"]
        },
        {
            "before": ["<Space>", "f", "r"],
            "commands": ["<Action>(RecentFiles)"]
        },
        {
            "before": ["<Space>", "v", "r", "n"],
            "commands": ["editor.action.rename"]
        },
        {
            "before": ["<Space>", "v", "c", "a"],
            "commands": ["<Action>(GotoAction)"]
        },
        {
            "before": ["g", "i"],
            "commands": ["references-view.findImplementations"]
        },
        {
            "before": ["g", "r"],
            "commands": ["editor.action.goToReferences"]
        },
        {
            "before": ["<Space>", "d", "b"],
            "commands": ["editor.debug.action.toggleBreakpoint"]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["J"],
            "commands": ["editor.action.moveLinesDownAction"]
        }, 
        {
            "before": ["K"],
            "commands": ["editor.action.moveLinesUpAction"]
        } 
        {
            "before": ["<Space>fg"],
            "commands": ["<Action>(FindInPath)"]
        },
        {
            "before": ["<Space>fc"],
            "commands": ["<Action>(GotoClass)"]
        },
        {
            "before": ["<Space>ff"],
            "commands": ["<Action>(GotoFile)"]
        },
    ],
}



